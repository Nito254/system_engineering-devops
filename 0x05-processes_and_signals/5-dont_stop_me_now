#!/usr/bin/env bash
# Find the PID of the process containing the name "4-to_infinity_and_beyond"
PID=$(pgrep -f "4-to_infinity_and_beyond")

# Check if the process was found
if [ -n "$PID" ]; then
  echo "Stopping 4-to_infinity_and_beyond (PID: $PID)..."
  kill "$PID"
else
  echo "Process not found: 4-to_infinity_and_beyond"
fi
